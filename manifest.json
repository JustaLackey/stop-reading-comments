{

  "manifest_version": 2,
  "name": "Stop Reading Comments",
  "version": "1.0",

  "description": "Blocks reddit comment threads",

  "icons": {
    "48": "icons/stopreading-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["stopreadingcomments.js"]
    }
  ]

}